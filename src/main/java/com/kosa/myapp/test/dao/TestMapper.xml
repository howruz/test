<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kosa.myapp.test.dao.ITestRepository">

	<select id="getList" resultType="com.kosa.myapp.test.model.Test">
		<![CDATA[
			SELECT userid AS "userId",
					name AS "name",
					password AS "email",
					phone AS "phone"
			FROM MEMBER_0
		]]>
	</select>





</mapper>